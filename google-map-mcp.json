{
	"servers": {
		"google-maps": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-google-maps@0.6.2"
			],
			"env": {
				"GOOGLE_MAPS_API_KEY": "${input:google_maps_api_key}"
			},
			"type": "stdio"
		}
	},
	"inputs": [
		{
			"id": "google_maps_api_key",
			"type": "promptString",
			"description": "Google Maps API Key"
		}
	]
}
